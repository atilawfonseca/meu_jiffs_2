<%- include('partials/header')  %>
<main class="mt-5 " >
    <h2 class="pt-5 mb-5">Digite seu login de acesso</h2>
    <div class="card p-3">
        <form action="/logar" method="post">
            <div class="input-group mb-3">
                <input type="text" name="coor_email" class="form-control" placeholder="Email" aria-label="Recipient's username" aria-describedby="basic-addon2">
                <span class="input-group-text" id="basic-addon2">@ifpa.edu.br</span>
            </div>
            <div class="input-group mb-3">
                <input name="senha" type="password" class="form-control" placeholder="Senha" aria-label="Recipient's username" aria-describedby="basic-addon2">
            </div>
            <button type="submit" class="btn btn-green">Acessar</button>
        </form>
    </div>
    <div class="mt-3 login-cadastro">
        <a href="/cadastrar" >Novo cadastro</a>
        <a href="/recuperar_senha" >Esqueci a senha</a>
    </div>
</main>
<%- include('partials/footer')  %>